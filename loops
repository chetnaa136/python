# i=1
# palindrome and reverse
# rev=[,,-1]
# if n==rev:
#     print("its a palindrome")
# else:
#     print("not a palindrome")
n=int(input("enter number"))
x=n
rev=0
while n>0:
    last=n%10
    rev=rev*10+last
    n=n//10
print(rev)
print(x)
if x==rev:
    print("it is a palindrome")
else:
    print("not a palindrome")
     